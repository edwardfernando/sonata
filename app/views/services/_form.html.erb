<%= form_for @service do |s| %>
  <p>
    <%= s.label :name %> </br>
    <%= s.text_field :name %>
  </p>
  <p>
    <%= s.label :description %> </br>
    <%= s.text_field :description %>
  </p>
  <p>
    <%= s.label :date %> </br>
    <%= s.text_field :date, :type => "date" %>
  </p>
  <p>
    <%= s.submit %>
  </p>
<% end %>
